            <Button
                android:id="@+id/filter_throttleWithtimeout_debounce"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="过滤操作符之throttleWithTimeout()/debounce()"
                android:textAllCaps="false"/>

            <Button
                android:id="@+id/filter_firstElement_lastElement"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="过滤操作符之firstElement()/lastElement()"
                android:textAllCaps="false"/>


            <Button
                android:id="@+id/filter_elementAt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="过滤操作符之elementAt()"
                android:textAllCaps="false"/>


            <Button
                android:id="@+id/filter_elementAtError"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="过滤操作符之elementAtError()"
                android:textAllCaps="false"/>

            <Button
                android:id="@+id/filter_ignoreelements"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
