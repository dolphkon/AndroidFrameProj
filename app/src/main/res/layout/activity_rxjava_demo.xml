            <Button
                android:id="@+id/func_retry"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="错误处理符之retry()"
                android:textAllCaps="false"/>


            <Button
                android:id="@+id/func_retryUntil"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="错误处理符之retryUntil()"
                android:textAllCaps="false"/>

            <Button
                android:id="@+id/func_retryWhen"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="错误处理符之retryWhen()"
                android:textAllCaps="false"/>


            <Button
                android:id="@+id/func_repeat"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="重复操作符之repeat()"
                android:textAllCaps="false"/>


            <Button
                android:id="@+id/func_repeatWhen"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="重复操作符之repeatWhen()"
                android:textAllCaps="false"/>


            <Button
                android:id="@+id/func_repeatUntil"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="重复操作符之repeatUntil()"
                android:textAllCaps="false"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:background="#f00"/>

            <Button
                android:id="@+id/filter_filter"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="过滤操作符之filter()"
                android:textAllCaps="false"/>


            <Button
                android:id="@+id/filter_ofType"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="过滤操作符之ofType()"
                android:textAllCaps="false"/>

            <Button
                android:id="@+id/filter_skip_skipLast"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="过滤操作符之skip() / skipLast()"
                android:textAllCaps="false"/>


            <Button
                android:id="@+id/filter_distinct_distinctUntilChanged"
                android:layout_width="wrap_content"
